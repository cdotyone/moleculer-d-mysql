{
  "name": "molculer-d-mysql",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "clean": "npx del-cli \"**/node_modules\" \"**/dist\" \"**/coverage\"",
    "dev": "moleculer-runner --repl --hot services/**/*.service.js --envfile .env.DEV",
    "prod": "moleculer-runner services/**/*.service.js --envfile .env.PROD",
    "start": "moleculer-runner --repl --hot services/**/*.service.js --envfile .env.LOCAL",
    "cli": "moleculer connect ",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microser
    vices",
    "moleculer"
  ],
  "license":"MIT",
  "repository": {"url": "https://github.com/cdotyone/moleculer-d-mysql.git"},
  "author": "https://github.com/cdotyone",
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.33",
    "moleculer-repl": "^0.7.4",
    "moleculer-web": "^0.10.7",
    "nodemailer": "^6.9.13",
    "pluralize": "^8.0.0",
    "qs": "^6.12.0",
    "sequelize": "^6.37.2",
    "swagger-ui-dist": "^5.14.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0"
  },
  "overrides": {
    "debug": "^4.3.4",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0"
  },
  "engines": {
    "node": ">= 16.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
